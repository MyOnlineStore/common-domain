{
    "name": "myonlinestore/common-domain",
    "type": "library",
    "description": "Common MyOnlineStore domain concepts",
    "homepage": "https://github.com/MyOnlineStore/common-domain",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "egulias/email-validator": "^2.1",
        "giggsey/libphonenumber-for-php": "^8.11",
        "layershifter/tld-extract": "^2.0",
        "league/uri": "^5.3",
        "litipk/php-bignumbers": "^0.8.6",
        "moneyphp/money": "^3.3",
        "ramsey/uuid": "^3.7",
        "ronanguilloux/isocodes": "^2.1"
    },
    "require-dev": {
        "doctrine/orm": "2.5.x-dev as 2.5.6",
        "myonlinestore/coding-standard": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.24"
        }
    }
}
