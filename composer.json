{
    "name": "myonlinestore/common-domain",
    "type": "library",
    "description": "Common MyOnlineStore domain concepts",
    "homepage": "https://github.com/MyOnlineStore/common-domain",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MyOnlineStore/litipk-php-exceptions"
        },
        {
            "type": "vcs",
            "url": "https://github.com/MyOnlineStore/php-bignumbers"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "litipk/php-bignumbers": "dev-fix-division-scale-guess as 0.7.3",
        "moneyphp/money": "^3.0",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "6.4.*",
        "escapestudios/symfony2-coding-standard": "^3.0",
        "doctrine/orm": "2.5.x-dev as 2.5.6"
    },
    "config": {
        "vendor-dir": "vendor",
        "sort-packages": true,
        "platform": {
            "php": "7.0.8"
        }
    }
}
