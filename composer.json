{
    "name": "myonlinestore/common-domain",
    "type": "library",
    "description": "Common MyOnlineStore domain concepts",
    "homepage": "https://github.com/MyOnlineStore/common-domain",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\Benchmark\\": "tests/benchmarks/",
            "MyOnlineStore\\Common\\Domain\\Tests\\": "tests/",
            "DragonBe\\Test\\Vies\\Validator\\": "vendor/dragonbe/vies/tests/Vies/Validator/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "dragonbe/vies": "^2.2",
        "egulias/email-validator": "^3.1",
        "giggsey/libphonenumber-for-php": "^8.12",
        "ramsey/uuid": "^4.1",
        "ronanguilloux/isocodes": "^2.3"
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^3.1",
        "phpbench/phpbench": "1.2.14",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.7"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0.3"
        }
    }
}
