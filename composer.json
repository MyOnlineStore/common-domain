{
    "name": "myonlinestore/common-domain",
    "type": "library",
    "description": "Common MyOnlineStore domain concepts",
    "homepage": "https://github.com/MyOnlineStore/common-domain",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\Benchmark\\": "tests/benchmarks/",
            "MyOnlineStore\\Common\\Domain\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true
        }
    },
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "egulias/email-validator": "^2.1 || ^3.1",
        "giggsey/libphonenumber-for-php": "^8.12",
        "jeremykendall/php-domain-parser": "^5.7",
        "league/uri": "^5.3",
        "litipk/php-bignumbers": "^0.8.6",
        "moneyphp/money": "^3.3",
        "ramsey/uuid": "^3.7 || ^4.0 <4.1",
        "ronanguilloux/isocodes": "^2.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "doctrine/orm": "^2.12",
        "myonlinestore/coding-standard": "^3.1",
        "phpbench/phpbench": "^1.2",
        "phpunit/phpunit": "^8.5",
        "psalm/plugin-phpunit": "^0.16",
        "vimeo/psalm": "^4.22"
    }
}
