{
    "name": "myonlinestore/common-domain",
    "type": "library",
    "description": "Common MyOnlineStore domain concepts",
    "homepage": "https://github.com/MyOnlineStore/common-domain",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\Common\\Domain\\Benchmark\\": "tests/benchmarks/",
            "MyOnlineStore\\Common\\Domain\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "egulias/email-validator": "^3.2",
        "giggsey/libphonenumber-for-php": "^8.12",
        "jeremykendall/php-domain-parser": "^6.1",
        "league/uri": "^6.8",
        "litipk/php-bignumbers": "^0.8.6",
        "moneyphp/money": "^4.0",
        "ramsey/uuid": "^4.5",
        "ronanguilloux/isocodes": "^2.3",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "doctrine/orm": "^2.13",
        "myonlinestore/coding-standard": "^4.0",
        "phpbench/phpbench": "^1.2",
        "phpunit/phpunit": "^10.0",
        "psalm/plugin-phpunit": "^0.18",
        "symfony/phpunit-bridge": "^6.1",
        "vimeo/psalm": "^5.6"
    }
}
